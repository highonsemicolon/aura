{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    },
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/app/",
      "env": { "PATH": "${env:PATH}" },
      "cwd": "${workspaceFolder}",
      "args": [],
      "showLog": true,
      "trace": "trace"
    }
  ]
}
